<openerp>
    <data>
        <record id="fpa_auxiliar_analitico_data" model="fpa.financial.reports">
            <field name="name">LIBRO AUXILIAR ANALITICO</field>
            <field name="categ">ana</field>
            <field name="action">action_fpa_auxiliar_analitico</field>
            <field name="model_wzr">fpa.auxiliar.analitico.wizard</field>
            <field name="model">fpa.auxiliar.analitico.line</field>
            <field name="formato">xlsx</field>
            <field name="template">financial_reports.template_report_auxiliar_analitico</field>
            <field name="tree">view_fpa_auxiliar_analitico_line_tree</field>
            <field name="domain">
                [('user_id','=',%s),('company_id','=',%s),('account_id','!=',False),('financial_id','=',%s)]
            </field>
            <field name="title_color">#2E86C1</field>
            <field name="view_color">#2E86C1</field>
            <field name="format_money">$#,##0</field>
            <field name="numeric">H,I,J,K</field>
            <field name="sign">True</field>
            <field name="type">sco</field>
            <field name="analytic_filter">True</field>
            <field name="account_filter">True</field>
            <field name="partner_filter">True</field>
            <field name="journal_filter">True</field>
            <field name="view">wizard_fpa_auxiliar_analitico_view</field>
            <field name="title">Codigo,Cuenta,Referencia,Tercero,Asiento,Cuenta analitica, Fecha,Saldo
                inicial,Debito,Credito,Saldo Final
            </field>
            <field name="consulta">select id from fpa_auxiliar_line al
                WHERE user_id = {user_id} AND company_id = {company_id} AND al.financial_id={financial_id}
                ORDER BY cuenta asc, partner_id asc NULLS FIRST
            </field>
            <field name="consulta_xlsx">select aa.code, COALESCE(it.value,aa.name) as cuenta, rp.ref,
                rp.name,al.asiento, aaa.name as analytic, fecha,al.amount_inicial,al.debit, al.credit, al.amount_final,
                al.bold
                from fpa_auxiliar_analitico_line al
                left join account_account aa on aa.id = al.account_id
                left join ir_translation it on it.name = 'account.account,name' and it.res_id = aa.id AND it.lang =
                '{lang}'
                inner join fpa_auxiliar_analitico a on a.id = al.encabezado_id
                left join account_analytic_account aaa on aaa.id = al.account_analytic_id
                left join res_partner rp on rp.id = al.partner_id
                WHERE a.user_id = {user_id} AND a.company_id = {company_id} AND al.financial_id = {financial_id}
                ORDER BY aa.code asc NULLS FIRST, al.nivel, al.partner_id
            </field>
        </record>

